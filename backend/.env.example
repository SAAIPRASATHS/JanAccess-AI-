# OpenAI API Key (required for AI features, app works without it using fallbacks)
OPENAI_API_KEY=your_openai_api_key_here

# Database URL (defaults to SQLite, change to PostgreSQL for production)
DATABASE_URL=sqlite:///./janaccess_ai.db

# Allowed frontend origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,https://janaccess-ai.vercel.app

# Frontend URL (used for CORS in production)
FRONTEND_URL=http://localhost:5173
